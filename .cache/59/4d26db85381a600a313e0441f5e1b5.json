ition: absolute;\n    display: flex;\n    animation: tick 30s linear infinite;\n\n    div.jhed {\n        display:flex;\n        align-items: center;\n        font-size:100vh;\n        line-height:100vh;\n        margin: 0;\n        padding: 0;\n        font-family: kaneda-gothic, sans-serif;\n        color: inherit;\n\n    }\n}\n\ndiv.summary{\n    position: absolute;\n    color: white;\n    font-family: neue-haas-grotesk-text;\n    font-weight: 400;\n    font-size: clamp(36px, 5vw, 5vw);\n    line-height: clamp(36px, 5vw, 5vw);\n    width: 60%;\n    font-style: italic;\n    top: 0;\n    left: 0; \n    margin: 0;\n    filter: url('#wavy');\n    z-index: 13;\n    transform: translate3d(0,0,0);\n\n}\n\ndiv.quotes {\n    width: 100%;\n    height: 100%;\n    position: absolute;\n    z-index: 12;\n}\n\ndiv.contact {\n    z-index: 13;\n    position: absolute;\n    bottom: 20px;\n    right: 20px;\n    z-index: 9999;\n    font-size: clamp(36px, 5vw, 5vw);\n    line-height: clamp(36px, 5vw, 5vw);\n    font-family: neue-haas-grotesk-text;\n    font-weight: 400;\n    font-style: italic;\n    display:flex;\n    flex-direction: column;\n    transform: translate3d(0,0,0);\n    \n    filter: url(#wavy);\n\n    a {\n        color: white;\n        margin-bottom: 10px;\n        text-align: right;\n    }\n}\n\n.noise-filter {\n    display:none;\n}\n\n.skewed {\n    transform: skew(-13deg);\n    transition: all 0.25s cubic-bezier(.6,.01,.06,.99);\n    animation: rainbow 1.5s infinite steps(1);\n}\n\n.cat {\n    height: clamp(60px, 7vw, 7vw);\n    width: auto;\n    position: absolute;\n    animation: spin 10s linear infinite;\n\n}\n\n.quote {\n    color: white;\n    position: absolute;\n    font-family: neue-haas-grotesk-text;\n    font-style: italic;\n    font-size: 1.75vw;\n    line-height: 2vw;\n    width: 20vw;\n}\n\n.smiley {\n    font-family: 'Helvetica';\n}\n\n\n@media only screen  and (max-width: 428px) {\n\n    .jhed {\n        font-size: 90vh;\n        line-height: 90vh;\n    }\n    .cat {\n        height: 75px;\n    }\n    div.summary {\n        width: 100%;\n        font-size: 48px;\n        line-height: 1;\n    }\n}\n\n.water-texture {\n    height: 0;\n}\n\n"},"lineCount":null}},"error":null,"hash":"57ac1c264ba389f19e732ffd8ed22d3e","cacheData":{"env":{}}}