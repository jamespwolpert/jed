<html>
    <head>
        <meta name="viewport" content="width=device-width,initial-scale=1.0">
        <link rel="stylesheet" href="https://use.typekit.net/nyf6uiv.css">
        <link rel="stylesheet" href="/index.HASH_REF_05623f49eeb737623cabcd842bf300de.css">
    </head>

    <body>
        <svg class="water-texture" xmlns="http://www.w3.org/2000/svg" width="100%" height="100%">
            <filter id="waterTexture">


                <feTurbulence result="undulation" numoctaves="2" basefrequency="0" seed="0" type="turbulence">
                
                    <animate attributename="baseFrequency" dur="25s" keysplines="
                            0.5 0 0.5 1;
                            0.5 0 0.5 1" keytimes="
                            0;
                            0.5;
                            1" calcmode="spline" values="
                            0.005,0.009; 
                            0.009,0.005; 
                            0.005,0.009" repeatcount="indefinite"></animate>
                
                </feTurbulence>
                    <feColorMatrix in="undulation" type="hueRotate" values="180">
                
                        <animate attributename="values" dur="1s" from="0" to="360" repeatcount="indefinite"></animate>
                    
                    </feColorMatrix>
                <feColorMatrix in="dist" result="circulation" type="matrix" values="4 0 0 0 1
                                        4 0 0 0 1
                                        4 0 0 0 1
                                        1 0 0 0 0   
                                        "></feColorMatrix>
                <feDisplacementMap in="SourceGraphic" in2="circulation" scale="10" result="dist"></feDisplacementMap>
                <feDisplacementMap in="dist" in2="undulation" scale="10" result="woah"></feDisplacementMap>
                </filter>
            </svg>
        
        <div class="content">
            <div class="track">
                <div class="ticker">
                    <div class="jhed">JHED <span class="exclaim">!</span><span class="smiley">&#9786;</span> </div>
                    <div class="jhed">JHED <span class="exclaim">!</span> <span class="smiley">&#9786;</span> </div>
                    <div class="jhed">JHED <span class="exclaim">!</span> <span class="smiley">&#9786;</span> </div>
                    <div class="jhed">JHED <span class="exclaim">!</span> <span class="smiley">&#9786;</span> </div>
                </div>
            </div>

            <div class="quotes"></div>
            <div class="summary">
                A super chill Scottsman who does live sound for bewildered Americans.
            </div>

            <div class="contact">
                <a href="mailto:jhedisdead@icloud.com">Electronic Mail</a>
                <a href="tel:717538-0464">Ye olde telephone</a>
            </div>
        </div>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.19.0/TweenMax.min.js"></script>
        <script src="/index.HASH_REF_62d5dab885897b04655082d4fd532818.js"></script>
    </body>

</html>








